<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">

<aside class="list-group">
    <form action="/" method="GET"> 
        <button class="list-group-item">
            <i class="fa fa-home" style="font-size: 1.4em"></i>
            <span>Home</span>
        </button>
    </form>
    <form action='/<%= user.name %>/resources' method="GET"> 
        <button class="list-group-item">
            <i class="fas fa-bookmark" style="font-size: 1em"></i>
            <span>Resources</span>
        </button>
    </form>
    <form action='/<%= user.name %>/categories' formmethod="GET">
        <button type="submit" class="list-group-item" name="name" value="Category 1">
            <i class="fa fa-archive"></i>
            <span>Category 1</span>
        </button>
    </form>
    <form action='/<%= user.name %>/categories' formmethod="GET">
        <button type="submit" class="list-group-item" name="name" value="Category 2">
            <i class="fa fa-archive"></i>
            <span>Category 2</span>
        </button>
    </form>
    <form action='/<%= user.name %>/categories' formmethod="GET">
        <button type="submit" class="list-group-item" name="name" value="Category 3">
            <i class="fa fa-archive"></i>
            <span>Category 3</span>
        </button>
    </form>
    <form action='/<%= user.name %>/categories' formmethod="GET">
        <button type="submit" class="list-group-item" name="name" value="Category 4">
            <i class="fa fa-archive"></i>
            <span>Category 4</span>
        </button>
    </form>
    <form action='/<%= user.name %>/categories' formmethod="GET">
        <button type="submit" class="list-group-item" name="name" value="Category 5">
            <i class="fa fa-archive"></i>
            <span>Category 5</span>
        </button>
    </form>
    <form action='/<%= user.name %>/liked-resources' method="GET"> 
        <button class="list-group-item">
            <i class="fa fa-thumbs-o-up" aria-hidden="true"></i>
            <span>Liked</span>
        </button>
    </form>
</aside>